set style line 1 lc rgb '#e6194b'   # --- blue
set style line 2 lc rgb '#3cb44b'   # --- red
set style line 3 lc rgb '#0082c8'   # --- blue
set style line 4 lc rgb '#f58231'   # --- red

set term png
set output 'output_chart1.png'

plot 'output.dat' using 1:2 title '1' with line ls 1, \
     'output.dat' using 1:3 title '2' with line ls 2, \
     'output.dat' using 1:4 title '3' with line ls 3, \
     'output.dat' using 1:5 title '4' with line ls 4

replot
set output 'output_chart2.png'

plot 'output.dat' using 1:6 title '7' with line ls 1, \
     'output.dat' using 1:8 title '8' with line ls 2, \
     'output.dat' using 1:9 title '9' with line ls 3, \
     'output.dat' using 1:10 title '10' with line ls 4

replot
set output 'output_chart3.png'

plot 'output.dat' using 1:11 title '11' with line ls 4

replot
set output 'output_chart4.png'

plot 'output.dat' using 1:7 title '7' with line ls 1